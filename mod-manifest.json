{
    "apiVersion": "2.0",
    "version": "1.0",
    "type": "action",
    "name": "Semiconductor",
    "id": "semiconductor",
    "description": "Transforms data and instantiates visualizations for semiconductor bin analysis.",
    "capabilities": [
        "LibraryRead"
    ],
    "icon": "icon.svg",
    "scripts": [
        {
            "id": "wafer-transform",
            "name": "Wafermap data transform",
            "file": "build/wafer-transform.js",
            "entryPoint": "applyWaferTransform",
            "parameters": [
                {
                    "name": "baseDataTable",
                    "displayName": "Base data table containing Circle, Segment, and Mask columns",
                    "type": "DataTable",
                    "description": "The data table to transform"
                }
            ]
        },
        {
            "id": "wafer-mapchart",
            "name": "Create wafer map chart",
            "file": "build/wafer-mapchart.js",
            "entryPoint": "createWaferMapchart",
            "parameters": [
            ]
        },
        {
            "id": "create-linechart",
            "name": "Create a LineChart",
            "entryPoint": "createLinechart",
            "file": "build/create-linechart.js"
        }
    ]
}
